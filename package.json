{
  "name": "gatsby-plugin-wordsby",
  "version": "0.0.6-beta.1",
  "description": "A Gatsby plugin for Wordsby/Wordpress",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-npm-files": "cp -rf package.json ./lib/ && cp -rf readme.md ./lib/",
    "clean-lib": "node_modules/.bin/rimraf ./lib",
    "build": "rm -rf src/package.json src/node_modules src/yarn.lock || true && yarn clean-lib && babel src --out-dir lib --ignore __tests__ && yarn copy-npm-files",
    "watch": "npm run clean-lib && mkdir lib && npm run copy-npm-files && babel -w src --out-dir lib --ignore __tests__"
  },
  "author": "Tyler Barnes",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.2.0",
    "babel-preset-gatsby-package": "^0.1.3",
    "better-react-spinkit": "^2.0.4",
    "gatsby-awesome-pagination": "^0.3.3",
    "glob": "^7.1.3",
    "import-cwd": "^2.1.0",
    "lodash": "^4.17.11",
    "node-fs": "^0.1.7",
    "rimraf": "^2.6.2"
  }
}
